{
  "name": "angular-enterprise",
  "displayName": "Angular Enterprise",
  "publisher": "philipgriffin",
  "icon": "icon.png",
  "description": "A curation of the best extensions to assist in building enterprise-grade Angular applications.",
  "repository": {
    "type": "git",
    "url": "https://github.com/philipgriffin/vscode-angular-enterprise.git"
  },
  "version": "15.0.2",
  "engines": {
    "vscode": "^1.73.0"
  },
  "scripts": {
    "build": "npx vsce package --out \"build/\"",
    "publish": "npx vsce publish"
  },
  "categories": [
    "Extension Packs"
  ],
  "keywords": [
    "Angular",
    "Nx",
    "TypeScript",
    "HTML"
  ],
  "license": "LicenseRef-LICENSE.md",
  "extensionPack": [
    "Angular.ng-template",
    "nrwl.angular-console",
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
    "ms-azuretools.vscode-docker",
    "pkief.material-icon-theme"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "angular-enterprise.helloWorld",
        "title": "Hello World"
      }
    ]
  }
}
